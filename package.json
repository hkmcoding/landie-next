{
  "name": "landie-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:setup": "playwright install",
    "test:supabase:start": "supabase start --ignore-health-check",
    "test:supabase:stop": "supabase stop",
    "test:supabase:reset": "supabase db reset --debug",
    "test:analytics": "vitest -c vitest.config.analytics.ts",
    "test:analytics:ui": "vitest -c vitest.config.analytics.ts --ui",
    "test:analytics:with-db": "yarn test:supabase:start && yarn test:analytics",
    "test:e2e:with-db": "yarn test:supabase:start && yarn test:e2e"
  },
  "dependencies": {
